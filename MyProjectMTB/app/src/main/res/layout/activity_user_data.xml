<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserData">

    <TextView
        android:id="@+id/tag_datos_usuario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Datos Usuario"
        android:textSize="40sp"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="71dp" />

    <TextView
        android:id="@+id/txt_data_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Identificacion"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="205dp" />

    <TextView
        android:id="@+id/txt_data_nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Nombre"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="268dp" />

    <TextView
        android:id="@+id/txt_data_apellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Apellido"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="336dp" />

    <TextView
        android:id="@+id/txt_data_correo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Correo"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="385dp" />

    <EditText
        android:id="@+id/input_buscar_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Identificacion"
        android:inputType="text"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="94dp"
        tools:layout_editor_absoluteY="532dp" />

    <Button
        android:id="@+id/btn_buscar_usuario"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Buscar"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="609dp" />

    <TextView
        android:id="@+id/txt_data_telefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Telefono"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="441dp" />

    <Button
        android:id="@+id/btn_volver_dashboard_1"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Volver"
        tools:layout_editor_absoluteX="294dp"
        tools:layout_editor_absoluteY="17dp" />
</androidx.constraintlayout.widget.ConstraintLayout>